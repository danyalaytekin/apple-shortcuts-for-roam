{
	"info": {
		"name": "Roam's web API",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get by id",
			"protocolProfileBehavior": {
				"disabledSystemHeaders": {
					"user-agent": true
				},
				"followAuthorizationHeader": true,
				"disableCookies": true
			},
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"eid\": \"[:block/uid \\\"BLOCK_ID\\\"]\",\n  \"selector\": \"[:block/string]\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "https://api.roamresearch.com/api/graph/{{GRAPH_NAME}}/pull",
					"protocol": "https",
					"host": [
						"api",
						"roamresearch",
						"com"
					],
					"path": [
						"api",
						"graph",
						"{{GRAPH_NAME}}",
						"pull"
					]
				}
			},
			"response": []
		}
	],
	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "GRAPH_API_KEY",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "GRAPH_NAME",
			"value": "some-graph-name",
			"type": "string"
		}
	]
}